      'where'
      '  ANO = :OLD_ANO and'
      '  CONTROLE = :OLD_CONTROLE and'
      '  ITEM = :OLD_ITEM')
    InsertSQL.Strings = (
      'insert into TBINVENTARIO_ALMOX_ITEM'
      
        '  (ANO, CONTROLE, CUSTO, ESTOQUE, FRACIONADOR, ITEM, LOTE_CONFER' +
        'IDO, LOTE_RESULTADO, '
