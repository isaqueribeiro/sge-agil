      '  LOTE_RESULTADO'
      'from TBINVENTARIO_ALMOX_ITEM '
      'where'
      '  ANO = :ANO and'
      '  CONTROLE = :CONTROLE and'
      '  ITEM = :ITEM')
    ModifySQL.Strings = (
      'update TBINVENTARIO_ALMOX_ITEM'
      'set'
      '  ANO = :ANO,'
      '  CONTROLE = :CONTROLE,'
      '  CUSTO = :CUSTO,'
      '  ESTOQUE = :ESTOQUE,'
